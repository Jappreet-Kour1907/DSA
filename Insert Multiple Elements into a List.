int main() {
    int n;
    scanf("%d", &n);
    int list[100];
    for(int i=0; i<n; i++){
        scanf("%d", &list[i]);
    }
    int k;
    scanf("%d", &k);
    for(int i=0; i<k; i++){
        int value, size;
        scanf("%d %d", &value, &size); 
        for(int j=n-1; j>=size; j--){
            list[j+1] = list[j];
        }
        list[size] = value;
        n++;
    }
    for(int i=0; i<n; i++){
        printf("%d ", list[i]);
    }
    return 0;
}
